<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jstech.fairy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".IsSecuredApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".Navigation.Navi_Security.Navi_Secuity" />
        <activity
            android:name=".Navigation.Navi_ContactUs" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".Navigation.Navi_Developers" />
        <activity android:name=".Navigation.Navi_LicenseInfo"></activity>

        <activity
            android:name="com.jstech.fairy.Navigation.Navi_Security.PasscodeManagePasswordActivity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" >
        </activity>

        <activity
            android:name="com.jstech.fairy.Navigation.Navi_Security.PasscodeUnlockActivity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" >
        </activity>



    </application>

</manifest>